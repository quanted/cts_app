<?xml version="1.0" encoding="UTF-8"?>
 
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.7"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.7
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.7.xsd">
    <changeSet id="1" author="chemaxon">
        <comment>Column labels</comment>
        <createTable tableName="columnnameLog">
        	<column name="id" type="id">
				<constraints primaryKey="true" nullable="false"/>
  			</column>
 			<column name="dbName" type="java.sql.Types.VARCHAR(255)">
				<constraints primaryKey="false" nullable="false"/>
  			</column>
 			<column name="tableName" type="java.sql.Types.VARCHAR(255)">
				<constraints primaryKey="false" nullable="false"/>
  			</column>
 			<column name="columnName" type="java.sql.Types.VARCHAR(255)">
				<constraints primaryKey="false" nullable="false"/>
  			</column>
 			<column name="columnLabel" type="java.sql.Types.CLOB">
				<constraints primaryKey="false" nullable="false"/>
  			</column>
        </createTable>
    </changeSet>
</databaseChangeLog>
